<!--authenticated.ejs (Authenticated interface)-->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Hello, <%= netid %>!</h1>

    <h2>You are successfully authenticated via CAS Authentication!</h2>

    <!-- available links -->
    <div style="font-family: Arial, Helvetica, sans-serif;">
        <a href="/logout/app">Log out of the app</a></br>
        <a href="/logout/cas">Log out of the app and end the CAS session</a></br>
    </div>

    </br>
    <h3>Note: Just logging out of the app will keep your CAS session active; however,
        your app session data will be cleared and revalidation of your CAS ticket will be needed.
    </h3>

</body>
</html>